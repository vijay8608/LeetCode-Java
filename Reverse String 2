class Solution {
    public String reverseString(String s) {
        
        char[] arr = s.toCharArray();
        char[] result = new char[arr.length];
        
        for(int i = arr.length - 1; i >= 0 ; i--){
            result[arr.length - 1 - i]=arr[i];
        }
        
        return new String(result);
          
    }
}